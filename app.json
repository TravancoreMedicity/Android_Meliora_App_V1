{
  "expo": {
    "name": "Meliora",
    "slug": "Meliora",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/c950e698-575e-4c32-a62d-6ed415f3c5ee"
    },
    "assetBundlePatterns": ["**/*"],
    "platforms": ["ios", "android", "web"],
    "newArchEnabled": true,
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#c9e265"
      },
      "package": "com.ajithajeesh.Meliora",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "notification": {
      "icon": "./assets/Notificon2.png",
      "color": "#edf2eb",
      "androidMode": "default",
      "androidCollapsedTitle": "You have new messages"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/Notificon2.png",
          "color": "#00AA00",
          "defaultChannel": "defaultExpoChannel",
          "sounds": ["./assets/pop_sound.wav", "./assets/bells_sound.wav"],
          "enableBackgroundRemoteNotifications": true
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "c950e698-575e-4c32-a62d-6ed415f3c5ee"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
